function getMobileOperatingSystem() {
  var userAgent = navigator.userAgent || navigator.vendor || window.opera;
  if (/windows phone/i.test(userAgent)) {
    return "Windows Phone";
  }
  if (/android/i.test(userAgent)) {
    return "Android";
  }
  if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
    return "iOS";
  }
  return "unknown";
}
let stopLoading = false;
let insertCSS = false;
let os = getMobileOperatingSystem();

if(window.location.href.endsWith("Members/Home")) {
    stopLoading = true;
    window.location.href = "https://testarea.pfpleisure.org/LhWebTest/##TARGET_URL##";
    insertCSS = true;
    console.log("insertCSS made true");
}

if(window.location.href.includes("##TARGET_URL##") && os == "iOS") {
    Interface.handleNativeAction(JSON.stringify({ "type": "stopLoading", "key": "val" }));
}

if(os != "iOS" && stopLoading) {
    Interface.handleNativeAction(JSON.stringify({ "type": "stopLoading", "key": "val", "delay":"2000" }));
}

if(window.location.href.endsWith("Members/Home") && os != "iOS") {
  console.log("inside css block 111");
  let navbar = document.getElementsByTagName('nav')[0];
    if(navbar?.style?.display) {
      navbar.style.cssText = 'display : none !important'
    }
}

if(window.location.href.includes("MemberProfile") && os != "iOS") {
    console.log("inside css block");
    let navbar = document.getElementsByTagName('nav')[0];
    if(navbar?.style?.display) {
      navbar.style.cssText = 'display : none !important';
    }
 }


